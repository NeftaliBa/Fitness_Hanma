<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="50dp"
                />
            <gradient
                android:startColor="#4B61FF"
                android:endColor="#00C4FF"
                android:angle="-45"
                />
        </shape>
    </item>
    <item
        android:start=  "5dp"
        android:end=    "5dp"
        android:bottom= "5dp"
        android:top=    "5dp"
        >
        <shape android:shape="rectangle">
            <corners
                android:radius="50dp"
                />
            <solid
                android:color="@color/white"/>
        </shape>
    </item>
</layer-list>



