<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.Principal.Adnavbar">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/navbaradminform">

        <ImageView
            android:id="@+id/clases"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="10dp"
            android:layout_weight="5"
            android:layout_gravity="center"
            android:src="@drawable/adnavbarclases" />

        <ImageView
            android:id="@+id/instructor"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="10dp"
            android:layout_weight="5"
            android:layout_gravity="center"
            android:src="@drawable/adnavbarinstructor" />
        <ImageView
            android:id="@+id/cliente"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="10dp"
            android:layout_weight="5"
            android:layout_gravity="center"
            android:src="@drawable/adnavbarcliente" />

        <ImageView
            android:id="@+id/personal"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="10dp"
            android:layout_weight="5"
            android:layout_gravity="center"
            android:src="@drawable/adnavbarpersonal"/>
    </LinearLayout>

</FrameLayout>