<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Instructor.fragments.Navbar">

    <LinearLayout
        android:layout_marginHorizontal="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="15dp"
        android:background="@drawable/ovalo_forma">
        <ImageView
            android:id="@+id/iconoUsuario"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginEnd="10dp"
            android:layout_marginStart="5dp"
            android:layout_marginVertical="10dp"
            android:layout_weight="5"
            android:src="@drawable/usuario" />
        <ImageView
            android:id="@+id/iconoCalendario"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginEnd="10dp"
            android:layout_marginStart="5dp"
            android:layout_marginVertical="10dp"
            android:layout_weight="5"
            android:src="@drawable/calendario" />
        <ImageView
            android:id="@+id/iconoSocial"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginEnd="10dp"
            android:layout_marginStart="5dp"
            android:layout_marginVertical="10dp"
            android:layout_weight="5"
            android:src="@drawable/charlar" />
        <ImageView
            android:id="@+id/iconoAjustes"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginEnd="10dp"
            android:layout_marginStart="5dp"
            android:layout_marginVertical="10dp"
            android:layout_weight="5"
            android:src="@drawable/ajustes"/>
    </LinearLayout>

</FrameLayout>