<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Instructor.fragments.Navbar">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/navbaradminform">

        <ImageView
            android:id="@+id/iconoUsuario"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_gravity="center"
            android:layout_weight="5"
            android:padding="10dp"
            android:src="@drawable/principal" />

        <ImageView
            android:id="@+id/iconoCalendario"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="10dp"
            android:layout_weight="5"
            android:layout_gravity="center"
            android:src="@drawable/admin_add_cla_clock" />
        <ImageView
            android:id="@+id/iconoSocial"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:padding="10dp"
            android:layout_weight="5"
            android:layout_gravity="center"
            android:src="@drawable/avisos" />
    </LinearLayout>

</FrameLayout>